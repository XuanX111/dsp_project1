   "metadata": {},
   "outputs": [],
   "source": [
    "# imports a library 'pandas', names it as 'pd'\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "from IPython.display import Image\n",
    "\n",
    "# enables inline plots, without it plots don't show up in the notebook\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 99,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv('/Users/weiguo/Desktop/dsp/projects/project1/Turnstile_Usage_Data2017.csv')\n",
    "#strip the potential spaces\n",
    "df.columns = df.columns.str.strip()\n",
    "dataparse= df['Date'] + ' ' + df['Time']\n",
    "df['dateParse'] = pd.to_datetime(dataparse, format = '%m/%d/%Y %H:%M:%S') # for challenge 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 100,
   "metadata": {},
   "outputs": [],
   "source": [
    " # create a new column get the day of the week\n",
    "df['Day_of_week']=df['dateParse'].dt.dayofweek\n",
    "del df['Date']\n",
    "del df['Time']\n",
    "# add new columns year month day  \n",
    "df['Date'] = [d.date() for d in df['dateParse']]\n",
    "df['Year'] = [d.year for d in df['Date']]\n",
    "df['Month'] = [d.month for d in df['Date']]\n",
    "df['Day'] = [d.day for d in df['Date']]\n",
    "df['Hour']=df['dateParse'].dt.hour"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 101,
   "metadata": {},
   "outputs": [],
   "source": [
    "# add new column season \n",
    "def seasonFun(mon):\n",
    "    month=int(mon)\n",
    "    if month>=3 and month<=5:\n",
    "        return 'Spring'\n",
    "    elif month>=6 and month<=8:\n",
    "        return 'Summer'\n",
    "    elif month>=9 and month<=11:\n",
    "        return 'Fall'\n",
    "    else:\n",
    "        return 'Winter'\n",
    "    \n",
    "df['Season']= [seasonFun(s) for s in df['Month']]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 102,
   "metadata": {},
   "outputs": [],
   "source": [
    "# add new column time of day\n",
    "def timeFun(time):\n",
    "    \n",
    "    if time>=5 and time<12:\n",
    "        return 'Morning'\n",
    "    elif time>=12 and time<17:\n",
    "        return 'Afternoon'\n",
    "    elif time>=17 and time<21:\n",
    "        return 'Evening'\n",
    "    else:\n",
    "        return 'Night'\n",
    "    \n",
    "df['Time_of_day']= [timeFun(t) for t in df['Hour']]"
   ]
  },
